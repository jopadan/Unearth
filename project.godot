[application]

config/name="Unearth"
run/main_scene="res://Scenes/Main.tscn"
run/delta_sync_after_draw=true
boot_splash/image="Skip-loading-screen"
config/icon="res://Art/icon/UnearthIcon256x256.png"

[autoload]

Slabs="*res://Autoload/Slabs.gd"
Random="*res://Autoload/Random.gd"
Constants="*res://Autoload/Constants.gd"
Cube="*res://Autoload/Cube.gd"
Settings="*res://Autoload/Settings.gd"
Things="*res://Autoload/Things.gd"
Nodelist="*res://Autoload/Nodelist.gd"
Utils="*res://Autoload/Utils.gd"
M="*res://Autoload/M.gd"
Slabset="*res://Autoload/Slabset.gd"
Columnset="*res://Autoload/Columnset.gd"
Version="*res://Autoload/Version.gd"
Graphics="*res://Autoload/Graphics.gd"
Names="*res://Autoload/Names.gd"

[debug]

gdscript/warnings/unused_argument=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/integer_division=false

[display]

window/size/height=576
window/size/borderless=true
window/dpi/allow_hidpi=true
window/energy_saving/keep_screen_on=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/CopyStackFrames/plugin.cfg", "res://addons/GDScriptMacros/plugin.cfg", "res://addons/IncludeOnExport/plugin.cfg", "res://addons/NodeScriptRenamer/plugin.cfg", "res://addons/SingletonScriptsShortcut/plugin.cfg", "res://addons/UnearthEditorPlugin/plugin.cfg", "res://addons/godot-plugin-refresher/plugin.cfg" )

[gd2cs]

config/ask_no_python=true
config/python="py"

